S3method(make.asr.joint, constrained)
S3method(make.asr.stoch, constrained)

## ASR/Mkn
S3method(asr.marginal, mkn)
S3method(asr.joint, mkn)
S3method(asr.stoch, mkn)

## ASR/BiSSE
S3method(asr.marginal, bisse)
S3method(asr.marginal, bisse.t)

S3method(make.asr.marginal, bisse)
S3method(make.asr.marginal, musse)

## ASR/MuSSE
S3method(asr.marginal, musse)

